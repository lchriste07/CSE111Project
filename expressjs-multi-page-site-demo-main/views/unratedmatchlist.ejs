<!DOCTYPE html>
<html lang="en">
<head>
  <title>Leaderboard Ranking</title>
</head>
<body>
    <div class="table-data">
<h2>List of All Unranked Matches</h2>
    <table border="1">
        <tr>
            <th>Index         |</th>
            <th>Match ID       | </th>
            <th>Opening       | </th>
            <th>White Player ID   |</th>
            <th>Black Player ID   |</th>
            <th>White Player Rating   |</th>
            <th>Black Player Rating    |</th>
            <th># of turns     |</th>
            <th>Match Result    | </th>
        </tr>
        
        <%
        if(index.length!=0){
        var i=1;
        index.forEach(function(data){
        %>
        <tr>
            <td><%=i; %></td>
            <td><%=data.un_match_id %></td>
            <td><%=data.un_opening %></td>
            <td><%=data.un_white_id %></td>
            <td><%=data.un_black_id %></td>
            <td><%=data.un_white_rating %></td>
            <td><%=data.un_black_rating %></td>
            <td><%=data.un_turns %></td>
            <td><%=data.un_match_res %></td>

            
        </tr>
        <%  i++; }) %>
        <% } else{ %>
            <tr>
                <td colspan="7">No Data Found</td>
            </tr>
        <% } %>
    </table>
    </div>
</body>
</html>