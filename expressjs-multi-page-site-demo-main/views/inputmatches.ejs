<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Edit Info</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3"
      crossorigin="anonymous"
    />
    <link
      href="//maxcdn.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css"
      rel="stylesheet"
      id="bootstrap-css"
    />
    <script
      src="//maxcdn.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js"
    ></script>
    <script
      src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"
    ></script>
    <link rel="stylesheet" type="text/css" href="style.css " media="screen" />
  </head>
  <body>
    <section class="vh-100 gradient-custom">
      <div class="container py-5 h-100">
        <div class="row justify-content-center align-items-center h-100">
          <div class="col-12 col-lg-9 col-xl-7">
            <div
              class="card shadow-2-strong card-registration"
              style="border-radius: 15px;"
            >
              <div class="card-body p-4 p-md-5">
                <h3
                  class="mb-4 pb-2 pb-md-0 mb-md-5 row justify-content-center"
                >Add Match </h3>
                <form action="/inputGame" method="post">

                  <div class="row">
                    <div class="col-md-6 mb-4">

                      <div class="form-outline">
                        <input
                          type="text"
                          id="ID"
                          class="form-control form-control-lg"
                          placeholder="Match ID"
                          name="ID"
                          
                        />
                        <label class="form-label" for="ID">Match ID</label>
                      </div>

                    </div>
                    <div class="col-md-6 mb-4">

                      <div class="form-outline">
                        <input
                          type="text"
                          id="opening"
                          class="form-control form-control-lg"
                          placeholder="Opening"
                          name="opening"
                          
                        />
                        <label class="form-label" for="DOB">Opening</label>
                      </div>

                    </div>
                  </div>

                  <div class="row">
                    <div class="col-md-6 mb-4 pb-2">
                      <div class="form-outline">
                        <input
                          type="text"
                          id="whiteid"
                          class="form-control form-control-lg"
                          placeholder="White ID"
                          name="whiteid"
                    
                        />
                        <label
                          class="form-label"
                          for="emailAddress"
                        >White ID </label>
                      </div>

                    </div>
                    <div class="col-md-6 mb-4 pb-2">

        
                    </div>
                  </div>
                  
                  <div class="row">
                    <div class="col-md-6 mb-4 pb-2">

                      <div class="form-outline">
                        <input
                          type="text"
                          id="blackid"
                          class="form-control form-control-lg"
                          placeholder="Black ID"
                          name="blackid"
                    
                        />
                        <label
                          class="form-label"
                          for="blackid"
                        >Black ID </label>
                      </div>

                    </div>
                    <div class="col-md-6 mb-4 pb-2">

        
                    </div>
                  </div>


                  <div class="row">
                    <div class="col-md-6 mb-4 pb-2">

                      <div class="form-outline">
                        <input
                          type="text"
                          id="whiterating"
                          class="form-control form-control-lg"
                          placeholder="White Rating"
                          name="whiterating"
                    
                        />
                        <label
                          class="form-label"
                          for="whiterating"
                        >White Rating </label>
                      </div>

                    </div>
                    <div class="col-md-6 mb-4 pb-2">

        
                    </div>
                  </div>

                  <div class="row">
                    <div class="col-md-6 mb-4 pb-2">

                      <div class="form-outline">
                        <input
                          type="text"
                          id="blackrating"
                          class="form-control form-control-lg"
                          placeholder="Black Rating"
                          name="blackrating"
                    
                        />
                        <label
                          class="form-label"
                          for="emailAddress"
                        >Black Rating </label>
                      </div>

                    </div>
                    <div class="col-md-6 mb-4 pb-2">

        
                    </div>
                  </div>

                  <div class="row">
                    <div class="col-md-6 mb-4 pb-2">

                      <div class="form-outline">
                        <input
                          type="text"
                          id="turncount"
                          class="form-control form-control-lg"
                          placeholder="Turn Count"
                          name="turncount"
                    
                        />
                        <label
                          class="form-label"
                          for="turncount"
                        >Turn Count </label>
                      </div>

                    </div>
                    <div class="col-md-6 mb-4 pb-2">

        
                    </div>
                  </div>

                  <div class="row">
                    <div class="col-md-6 mb-4 pb-2">

                      <div class="form-outline">
                        <input
                          type="text"
                          id="movelist"
                          class="form-control form-control-lg"
                          placeholder="Move List"
                          name="movelist"
                    
                        />
                        <label
                          class="form-label"
                          for="movelist"
                        >Move List</label>
                      </div>

                    </div>
                    <div class="col-md-6 mb-4 pb-2">

        
                    </div>
                  </div>


                  <div class="row">
                    <div class="col-md-6 mb-4 pb-2">

                      <div class="form-outline">
                        <input
                          type="text"
                          id="matchres"
                          class="form-control form-control-lg"
                          placeholder="Match Result"
                          name="matchres"
                    
                        />
                        <label
                          class="form-label"
                          for="matchres"
                        >Match Result </label>
                      </div>

                    </div>
                    <div class="col-md-6 mb-4 pb-2">

        
                    </div>
                  </div>




                  <div class="mt-4 pt-2">
                    <input
                      class="btn btn-primary btn-lg"
                      type="submit"
                      value="Save Match"
                    />
                    <br />
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
  </body>

</html>